int main()
{
/*5-setTokenAt(string& s, char sep, string t, int i)*/
   string s = "John|Paul|George|Ringo";
   char sep = '|';
   string t = "Meli";
   int i = 0;
   setTokenAt(s, sep, t, i);
   cout << s << endl; //Out: John|Paul|Ringo

  /* i = 0;
