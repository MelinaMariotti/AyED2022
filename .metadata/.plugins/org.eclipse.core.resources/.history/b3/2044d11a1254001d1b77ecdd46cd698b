   //creamos un nodo vacio
   Node<int>* p = NULL;

   add<int>(p,3);
   add<int>(p,1);
   add<int>(p,2);
   add<int>(p,3);
   add<int>(p,2);
   add<int>(p,1); // 3 1 2 3 2 1

