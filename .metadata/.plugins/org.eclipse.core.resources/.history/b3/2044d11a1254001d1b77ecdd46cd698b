void mostrar(Node<int>* p){

   Node<int>* aux = p;
   while ( aux != NULL){
      int inf = aux->info;
      cout << inf ;
      aux = aux->sig;
   }
}

int cmpIntDes(int a, int b){
   return b-a;
}

