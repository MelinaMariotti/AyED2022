
#ifndef _MAIN
#define _MAIN

#include <iostream>
#include "biblioteca/funciones/strings.hpp"
#include "biblioteca/funciones/tokens.hpp"
#include "biblioteca/funciones/files.hpp"
#include "biblioteca/tads/Array.hpp"
#include "biblioteca/tads/Map.hpp"
#include "biblioteca/tads/List.hpp"
#include "biblioteca/tads/Stack.hpp"
#include "biblioteca/tads/Queue.hpp"
#include "biblioteca/tads/Coll.hpp"
using namespace std;


int main()
{
     Array<string> arr = array<string>();
     arrayAdd<string>(arr, "Meli");
     arrayAdd<string>(arr, "Gon");
     arrayAdd<string>(arr, "Uriel");

     for(int i=0; i<arraySize<string>(arr); i++){
        string* p = arrayGet<string>(arr, i);
        cout << *p << endl;
     }

     arrayRemove<string>(arr, 2);

     cout << "Array with the 2nd element removed" << endl;
     for(int i=0; i<arraySize<string>(arr); i++){
        string* p1 = arrayGet<string>(arr, i);
        cout << *p1 << endl;
     }

     arrayAdd<string>(arr, "Loli");
     arrayAdd<string>(arr, "Estu");
     arrayRemove(arr, 0);

   return 0;
} //END MAIN

#endif
